{

 "apps": [

  {

   "name": "underfoot",

   "script": "app.js",

   "log_date_format": "YYYY-MM-DD HH:mm:SS",

   "log_file": "logs/underfoot.log",

   "error_file": "logs/underfoot-err.log",

   "out_file": "logs/underfoot-out.log",

   "pid_file": "pids/underfoot.pid",

   "instances": 1,

   "min_uptime": "20000s",

   "max_restarts": 10000,

   "max_memory_restart": "300M",

   "cron_restart": "1 0 * * *",

   "watch": false,

   "merge_logs": true,

   "exec_interpreter": "node",

   "exec_mode": "cluster_mode",

   "autorestart": true,

   "vizion": false

  }

 ]

}
